{
    "name": "linna/framework",
    "description": "Simple and elegant tools for build web application.",
    "keywords": [
        "best-pratices",
        "framework",
        "learning",
        "mvc-framework",
        "php"
    ],
    "homepage": "https://github.com/linna",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Rapetti",
            "email": "sebastian.rapetti@tim.it"
        }
    ],
    "require": {
        "linna/typed-array" :  "^1.0",
        "php": "^7.4",
        "psr/container": "^1.0",
        "s3b4stian/simple-cache": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.15",
        "linna/auth-mapper-mysql": "dev-master",
        "linna/db-dumps": "dev-master",
        "mongodb/mongodb": "^1.5",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Linna\\": "src/Linna/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Linna\\Tests\\":  ["tests/", "tests/Cache", "tests/Session"],
            "Linna\\TestHelper\\": "tests/TestHelper/"
        }
    }
}
